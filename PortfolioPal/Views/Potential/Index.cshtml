<div id="Suggested Trade Assets">
    <h3>Suggested Trade Assets</h3>
    <table class="table" id="suggested-trade-assets">
        <tr>
            <th>Score</th>
            <th>Symbol</th>
            <th>Price</th>
            <th>Company</th>
            <th>MarketCap</th>
            <th>1YrH</th>
            <th>1YrL</th>
            <th>30DAvgV</th>
            <th>EPS</th>
            <th>DivRate</th>
            <th>DivYield</th>
            <th>PE</th>
            <th>beta</th>
            <th>5Yr%Chg</th>
            <th>1Yr%Chg</th>
            <th>6M%Chg</th>
            <th>3M%Chg</th>
            <th>1M%Chg</th>
            <th>5D%Chg</th>
            <th>Exchange</th>
            <th>Class</th>
            <th>Updated</th>
        </tr>
        @foreach (var m in Model.SuggestedTradeAssets)
        {
            <tr>
                <td>@m.score</td>
                <td>@m.symbol</td>
                <td>@m.price</td>
                <td>@m.companyName</td>
                <td>@m.marketCap</td>
                <td>@m.week52High</td>
                <td>@m.week52Low</td>
                <td>@m.avg30Volume</td>
                <td>@m.ttmEPS</td>
                <td>@m.ttmDividendRate</td>
                <td>@m.dividendYield</td>
                <td>@m.peRatio</td>
                <td>@m.beta</td>
                <td>@m.year5ChangePercent</td>
                <td>@m.year1ChangePercent</td>
                <td>@m.month6ChangePercent</td>
                <td>@m.month3ChangePercent</td>
                <td>@m.month1ChangePercent</td>
                <td>@m.day5ChangePercent</td>
                <td>@m.exchange</td>
                <td>@m.assetClass</td>
                <td>@m.updated</td>
            </tr>
        }
    </table>
</div>

<div id="Suggested Dividend Assets">
    <h3>Suggested Dividend Assets</h3>
    <table class="table" id="suggested-dividend-assets">
        <tr>
            <th>Score</th>
            <th>Symbol</th>
            <th>Price</th>
            <th>Company</th>
            <th>MarketCap</th>
            <th>1YrH</th>
            <th>1YrL</th>
            <th>30DAvgV</th>
            <th>EPS</th>
            <th>DivRate</th>
            <th>DivYield</th>
            <th>nextDiv</th>
            <th>LastDiv</th>
            <th>Divfreq</th>
            <th>PE</th>
            <th>beta</th>
            <th>5Yr%Chg</th>
            <th>1Yr%Chg</th>
            <th>6M%Chg</th>
            <th>3M%Chg</th>
            <th>1M%Chg</th>
            <th>5D%Chg</th>
            <th>Exchange</th>
            <th>Class</th>
            <th>Updated</th>
        </tr>
        @foreach (var m in Model.SuggestedDividendAssets)
        {
            <tr>
                <td>@m.score</td>
                <td>@m.symbol</td>
                <td>@m.price</td>
                <td>@m.companyName</td>
                <td>@m.marketCap</td>
                <td>@m.week52High</td>
                <td>@m.week52Low</td>
                <td>@m.avg30Volume</td>
                <td>@m.ttmEPS</td>
                <td>@m.ttmDividendRate</td>
                <td>@m.dividendYield</td>
                <td>@m.nextDividendDate</td>
                <td>@m.exDividendDate</td>
                <td>@m.dividendFrequency</td>
                <td>@m.peRatio</td>
                <td>@m.beta</td>
                <td>@m.year5ChangePercent</td>
                <td>@m.year1ChangePercent</td>
                <td>@m.month6ChangePercent</td>
                <td>@m.month3ChangePercent</td>
                <td>@m.month1ChangePercent</td>
                <td>@m.day5ChangePercent</td>
                <td>@m.exchange</td>
                <td>@m.assetClass</td>
                <td>@m.updated</td>
            </tr>
        }
    </table>

</div>








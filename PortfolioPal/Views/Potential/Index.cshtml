@model PortfolioPal.ViewModels.ScreenerIndexVM

<h2>Portfolio Screener</h2>
<h3>Overview</h3>
<ul>
    <li><a class="testes" asp-area="" asp-controller="Potential" asp-action="Index">Overview</a></li>
    <li><a class="testes" asp-area="" asp-controller="Potential" asp-action="GetMoreData">Get More Data</a></li>
    <li><a class="testes" asp-area="" asp-controller="Potential" asp-action="Screen">Screen New Data</a></li>
    <li><a class="testes" asp-area="" asp-controller="Potential" asp-action="DisplayAllData">Display All Data</a></li>
    <li><a class="testes" asp-area="" asp-controller="Potential" asp-action="InitDB">Initialize Data</a></li>
</ul>
   
<div id = "table-summaries">
    <table class="table-tiny table-striped responsive">
        <tr>
            <th>Updated Assets</th>
            <th>User Selected Assets</th>
            <th>Potential Assets</th>
            <th>Total Assets</th>
        </tr>
        <tr>
            <td>@Model.NumAllUpdatedAssets</td>
            <td>@Model.NumUserSelectedAssets</td>
            <td>@Model.NumAllFilteredAssets</td>
            <td>@Model.NumAllAvailableAssets</td>
        </tr>
    </table>
</div>
